  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "http-proxy-middleware": "^0.17.4",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^5.5.0"
